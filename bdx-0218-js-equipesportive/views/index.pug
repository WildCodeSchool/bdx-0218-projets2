                        .photo-caption
                          .flag
                            img(src= staffs[y].nationnalité, alt='')
                          .caption
                            figcaption #{staffs[y].nom}
                          .jersey
                    -y++
                  else
                    .carousel-item
                      .photo-container
                        .photo-background
                        a(data-toggle='modal', data-target='#staff' + y +'-modal')
                          img(src= staffs[y].photo, alt='')
                        .photo-caption
                          .flag
                            img(src= staffs[y].nationnalité, alt='')
                          .caption
                            figcaption #{staffs[y].nom}
                          .jersey
                    -y++

            a.left.carousel-control-prev(href="#staff-carousel", role="button", data-slide="prev")
                  span.carousel-control-prev-icon
            a.right.carousel-control-next(href="#staff-carousel", role="button", data-slide="next")
