section.team-gallery#team
    h2 Galerie des joueurs
    ul#teamGallery.nav.nav-tabs(role='tablist')
      li.nav-item
        a#players-tab.nav-link(data-length = joueurs.length , data-toggle='tab', href='#players', role='tab', aria-controls='players', aria-selected='true') Les Joueurs
      li.nav-item
        a#staff-tab.nav-link(data-toggle='tab', href='#staff', role='tab', aria-controls='staff', aria-selected='false') Le Staff
    #teamGalleryContent.tab-content
      #players.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='players-tab')
        
        .container.d-none.d-lg-block
          
          .row            
            - for (let x = 0; x < joueurs.length; x++)
              -let modalTag = 'player' + x +'-modal';                
              .col-xs-1.col-md-6.col-lg-3
                .photo-container
                    .photo-background
                    a(data-toggle='modal', data-target='#player' + x +'-modal')
                      img(src= joueurs[x].photo, alt='Photo de #{joueurs[x].prenom} #{joueurs[x].nom}')
                    .photo-caption
                      .flag
                        img(src= joueurs[x].flag, alt='Drapeau')
                      .caption
                        figcaption #{joueurs[x].prenom} #{joueurs[x].nom} 
                      .jersey
                        p #1          
              


        .container.d-md-block.d-lg-none
          #header-carousel.carousel.slide(data-ride="carousel",data-interval="4000")
            .carousel-inner
              -var x = 0
              -let modalTag = 'player' + x +'-modal'; 
              for joueur in joueurs
                  if x == 0
                    .carousel-item.active
                      .photo-container
                        .photo-background
                        a(data-toggle='modal', data-target='#player' + x +'-modal')
                          img(src= joueurs[x].photo, alt='Photo de #{joueurs[x].prenom} #{joueurs[x].nom}')
                        .photo-caption
                          .flag
                            img(src= joueurs[x].flag, alt='Drapeau')
                          .caption
                            figcaption #{joueurs[x].prenom} #{joueurs[x].nom} 
                          .jersey
                            p #1    
                    -x++
                  else
                    .carousel-item
                      .photo-container
                        .photo-background
                        a(data-toggle='modal', data-target='#player' + x +'-modal')
                          img(src= joueurs[x].photo, alt='Photo de #{joueurs[x].prenom} #{joueurs[x].nom}')
                        .photo-caption
                          .flag
                            img(src= joueurs[x].flag, alt='Drapeau')
                          .caption
                            figcaption #{joueurs[x].prenom} #{joueurs[x].nom} 
                          .jersey
                            p #1
                    -x++

            a.left.carousel-control-prev(href="#header-carousel", role="button", data-slide="prev")
                  span.carousel-control-prev-icon
            a.right.carousel-control-next(href="#header-carousel", role="button", data-slide="next")
                  span.carousel-control-next-icon

      include ./modal.pug
      



      #staff.tab-pane.fade(role='tabpanel', aria-labelledby='staff-tab')
        .container.d-none.d-lg-block
          .row 
            - for (let y = 0; y < staffs.length; y++)
